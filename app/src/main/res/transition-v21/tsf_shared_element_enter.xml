<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
    android:interpolator="@android:interpolator/decelerate_cubic"
    android:transitionOrdering="together"
    >
  <changeBounds/>
  <changeTransform/>
  <changeImageTransform/>
  <arcMotion
      android:maximumAngle="90"
      android:minimumHorizontalAngle="60"
      android:minimumVerticalAngle="60"
      />
  <targets>
    <target android:targetName="@string/transition_name_circle_view"/>

    <target android:excludeName="@string/transition_name_fab"/>
    <target android:excludeId="@string/transition_name_fab"/>
  </targets>
</transitionSet>
